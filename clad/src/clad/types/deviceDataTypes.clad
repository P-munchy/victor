/** Clad message definitions for types of device data.
 */


namespace Anki {
namespace Vector {

  enum uint_8 DeviceDataType
  {
    DeviceID        = 0x00,
    DeviceModel,
    AppRunID,
    LastAppRunID,
    BuildVersion
  }

  structure DeviceDataPair
  {
    DeviceDataType dataType,
    string dataValue
  }
      
} // namespace Vector
} // namespace Anki
