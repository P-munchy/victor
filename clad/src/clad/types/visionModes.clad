/** Clad message definitions for the messages sent from the WiFi to the engine.
 */


namespace Anki {
namespace Cozmo {

  enum uint_8 VisionMode
  {
    Idle                   = 0x00,
    DetectingMarkers       = 0x01,
    Tracking               = 0x02,
    DetectingFaces         = 0x04,
    DetectingMotion        = 0x08,
    DetectingOverheadEdges = 0x10,
    ReadingToolCode        = 0x20,
    ComputingCalibration   = 0x40
  }
      
} // namespace Cozmo
} // namespace Anki
