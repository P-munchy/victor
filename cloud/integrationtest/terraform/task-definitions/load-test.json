[
    {
        "environment": [
            {
                "name": "ENVIRONMENT",
                "value": "loadtest"
            },
            {
                "name": "AWS_REGION",
                "value": "${region}"
            },
            {
                "name": "LOG_TYPE",
                "value": "kinesis"
            },
            {
                "name": "LOG_KINESIS_STREAM",
                "value": "splunk_logs_development"
            },
            {
                "name": "LOG_KINESIS_ROLE_ARN",
                "value": "${logging_role}"
            },
            {
                "name": "LOG_SOURCE",
                "value": "sai_loadtest"
            },
            {
                "name": "LOG_SOURCETYPE",
                "value": "sai_go_general"
            },
            {
                "name": "LOG_INDEX",
                "value": "sai_loadtest"
            },
            {
                "name": "ENABLE_ACCOUNT_CREATION",
                "value": "${enable_account_creation}"
            },
            {
                "name": "ENABLE_DISTRIBUTED_CONTROL",
                "value": "${enable_distributed_control}"
            },
            {
                "name": "REDIS_ADDRESS",
                "value": "redis.loadtest:6379"
            }
        ],
        "name": "load_test",
        "image": "${image}",
        "cpu": ${cpu},
        "memory": ${memory},
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "awslogs-load-test",
                "awslogs-region": "${region}",
                "awslogs-stream-prefix": "awslogs-load-test"
            }
        }
    }
  ]