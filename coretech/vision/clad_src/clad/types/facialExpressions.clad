/** Clad enumeration for the types of facial expressions we can detect
 */


namespace Anki {
namespace Vision {

enum int_8 FacialExpression
{
  Unknown = -1,    // e.g. expression estimation disabled

  // OKAO supports these
  Neutral = 0,
  Happiness,
  Surprise,
  Anger,
  Sadness,

  // Microsoft Cloud adds these
  Contempt,
  Disgust,
  Fear,

  Count
}

} // namespace Vision
} // namespace Anki
