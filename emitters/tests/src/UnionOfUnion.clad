// containing a union with union members

#include "Foo.clad"
#include "Bar.clad"

union FooBarUnion {
  Foo myFoo,
  Bar myBar
}

union BarFooUnion {
  Bar myBar,
  Foo myFoo
}

union UnionOfUnion {
  FooBarUnion myFooBar,
  BarFooUnion myBarFoo
}

message MessageOfUnion {
  int_32 anInt,
  FooBarUnion myFooBar,
  bool aBool
}

union fixedSizeNativeTypeUnion {
  float_64 varDouble,
  uint_64  varUint
}
