{
  // Example: define the channel name and whether it's enabled or disabled. 'enabled=true' means its logs will print,
  // while 'enabled=false' means they will be filtered out, and won't show up in the console.
  //    {
  //      "channel": "**",
  //      "enabled": true/false
  //    },

  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  //
  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  "osx" :
  {
    "channels":
    [
      { "channel": "Unnamed" ,        "enabled": false }, // Unnamed is currently the default channel
      { "channel": "CoreTech",        "enabled": false },
      { "channel": "Unfiltered",      "enabled": true  }, // things the are required always (like callstack dump on assert)
      { "channel": "CpuProfiler",     "enabled": true  },
      { "channel": "Keyboard",        "enabled": true },
      { "channel": "Network",         "enabled": false },
      { "channel": "Profiler",        "enabled": false },
      { "channel": "Unity"   ,        "enabled": false },
      { "channel": "VisionComponent", "enabled": false },
      { "channel": "VisionSystem",    "enabled": false },
      { "channel": "UnlockComponent", "enabled": false },
      { "channel": "Actions",         "enabled": false },
      { "channel": "Behaviors",       "enabled": false },
      { "channel": "AIWhiteboard",    "enabled": false },
      { "channel": "Audio",           "enabled": false },
      { "channel": "FaceRecognizer",  "enabled": false },
      { "channel": "UiComms",         "enabled": false },
      { "channel": "Mood",            "enabled": false },
      { "channel": "PoseConfirmer",   "enabled": false },
      { "channel": "BlockWorld",      "enabled": false }

    ],
    "levels":
    [
      { "level" : "event", "enabled" : false },
      { "level" : "debug", "enabled" : false }
    ]
  },
  
  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  //
  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  "android" :
  {
    "channels":
    [
      { "channel": "Unnamed",         "enabled": false }, // Unnamed is currently the default channel
      { "channel": "Unity"  ,         "enabled": false },
      { "channel": "Audio",           "enabled": false }
    ],
    "levels":
    [
      { "level" : "event", "enabled" : true },
      { "level" : "debug", "enabled" : false }
    ]
  },
  
  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  //
  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  "ios" :
  {
    "channels":
    [
      { "channel": "Unnamed",         "enabled": false }, // Unnamed is currently the default channel
      { "channel": "Unity"  ,         "enabled": false },
      { "channel": "Audio",           "enabled": false }
    ],
    "levels":
    [
      { "level" : "event", "enabled" : true },
      { "level" : "debug", "enabled" : false }
    ]
  }

}
