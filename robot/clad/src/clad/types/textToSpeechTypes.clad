namespace Anki {
namespace Cozmo {

  // Text To Speech processing state
  enum uint_8 TextToSpeechState
  {
    Invalid = 0,   // Initial state
    Preparing = 1, // Preparing TTS audio
    Ready = 2,     // Ready to play TTS audio
    Playing = 3,   // Playing TTS audio
    Done = 4,      // TTS audio complete
  }

} // namespace Cozmo
} // namespace Anki
