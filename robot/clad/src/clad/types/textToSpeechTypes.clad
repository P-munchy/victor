namespace Anki {
namespace Cozmo {

  // Text To Speech processing state
  enum uint_8 TextToSpeechState
  {
    Invalid = 0,  // Initial state
    Starting = 1, // TTS startup
    Running = 2,  // TTS in progress
    Done = 3,     // TTS complete
  }

} // namespace Cozmo
} // namespace Anki
