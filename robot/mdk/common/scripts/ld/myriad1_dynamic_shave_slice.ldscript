/*Linker script file which sets all dynamic code*/
  
SECTIONS {
  
  /*Setting up shave slices memory with absolute addresses*/
  . = 0x1D000000;
  .dyn.text : { 
        *(S.text*)
        *(S.__TEXT__sect)
        *(S.__MAIN__sect) 
        *(S.init*)
  }
  . = 0x1C000000; 
  .dyn.data : { 
        *(S.data*)
        *(S.rodata*)
        *(S.__DATA__sect*)
        *(S.__STACK__sect*)
        *(S.__static_data*)
        *(S.__HEAP__sect*)
        *(S.__T__*)
  }
}
