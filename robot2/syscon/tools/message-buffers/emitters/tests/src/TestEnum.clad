// A sample .clad file
// containing enums

enum no_cpp_class uint_32 FooEnum {
  foo1,
  foo2 = verbatim 2 * 4, //8
  foo3,
  foo4 = 10,
  foo5 = 0x500, //1280
  foo6,
  foo7 = verbatim 1000
}

enum uint_32 BarEnum {
  bar1,
  bar2 = verbatim FooEnum::foo2, //8
  bar3,
  bar4 = verbatim FooEnum::foo4 + FooEnum::foo6, //1281 + 10 = 1291
  bar5 = verbatim bar3 + 7, //9 + 7 = 16
  bar6
}
