    "configurations": [
        {
            "name": "engine:dbg",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/_build/android/Debug/bin/vic-engine",
            "env": {
                "LD_LIBRARY_PATH": "/anki/lib",
                "VIC_ENGINE_CONFIG": "/anki/etc/config/platform_config.json"
            },
            "stopOnEntry": false,
            "initCommands": [
                "platform select remote-android",
                "platform connect connect://localhost:55001",
                "platform shell systemctl stop vic-engine || true",
                "settings set target.exec-search-paths ${workspaceRoot}/_build/android/Debug/lib"
            ]
        },
        {
            "name": "engine:dbg:attach",
            "type": "lldb",
