{
    "version": "0.2.0",
    "compounds": [
        {
            "name": "victor:dbg:attach",
            "configurations": [
                "robot:dbg:attach",
                "anim:dbg:attach",
                "engine:dbg:attach"
            ]
        },
        {
            "name": "victor:rel:attach",
            "configurations": [
                "robot:rel:attach",
                "anim:rel:attach",
                "engine:rel:attach"
